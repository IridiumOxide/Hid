/* Variables can be redeclared in the same scope, or overshadowed in inner scope. */

bool a;
a = !!!a;
print a;       // True will be printed
int a;
print a;       // 0 will be printed
a = ^^^^^a;
print a;       // 5 will be printed
{
    int a;
    a = vvva;
    print a;   // -3 will be printed
}
print a;       // 5 will be printed
